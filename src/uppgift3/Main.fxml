<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uppgift3.MainController">
   <children>
      <HBox alignment="CENTER" layoutX="25.0" layoutY="316.0" prefHeight="70.0" prefWidth="350.0" spacing="10.0">
         <children>
            <Button fx:id="btnStartStop" mnemonicParsing="false" prefHeight="60.0" prefWidth="150.0" text="Start/Stop">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnExit" layoutX="136.0" layoutY="16.0" mnemonicParsing="false" onAction="#exit" prefHeight="60.0" prefWidth="100.0" text="Exit">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Label fx:id="lblTime" alignment="CENTER" layoutX="25.0" layoutY="54.0" prefHeight="100.0" prefWidth="350.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
